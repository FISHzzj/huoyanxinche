<!--pages/demo/IDcard.wxml-->
<view>
  <view class="grace-idcard-main">
    <view  style='display:inline;'>
      身份证照片 ( 正面 )
    </view><view  style='float:right'>
      身份证照片 ( 背面 )
    </view>
    <view class="grace-idcard-items" style="width:700rpx;" >
      <view class="grace-idcard-preview" style="display:inline;">
        <image  src="{{idCard1}}" bindtap="selectImg1" style='width:290rpx;height:99px'></image>
      </view>                   
      <view class="grace-idcard-preview" style="display:inline;">
        <image  src="{{idCard2}}" bindtap="selectImg2" style='width:290rpx;height:99px'></image>
      </view>
    </view>
  </view>
</view>